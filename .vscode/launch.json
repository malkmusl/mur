{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Flutter",
        "request": "launch",
        "type": "dart",
        "flutterMode": "debug",
        "program": "lib/main.dart",
        "args": [
          "--no-sound-null-safety"
        ],
        "cwd": "${workspaceFolder}",
        "stopOnEntry": false,
        "flutterRunLogFile": "flutter-logs.txt",
        "sourceMaps": true,
        "runOnce": true
      }
    ]
  }
  
